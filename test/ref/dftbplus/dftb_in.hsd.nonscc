Geometry = GenFormat {
  <<< "geo.gen"
}

Driver {}

Hamiltonian = DFTB {
  Scc = No
  MaxAngularMomentum {
    H="s"
    C="p"
  }
  Charge = 0.0
  Eigensolver = DivideAndConquer {}
  Filling = Fermi {
    Temperature [Kelvin] = 0.0
  }
  SlaterKosterFiles = Type2FileNames {
    Separator = "-"
    Suffix = ".skf"
  }

}

ParserOptions {
  ParserVersion = 5
}

Parallel {
UseOmpThreads = Yes
}

