Geometry = GenFormat {
  <<< "geo.gen"
}

Driver {}

Hamiltonian = DFTB {
  Scc = No
  MaxAngularMomentum {
    H="s"
  }
  Charge = 0.0
  SpinPolarisation {}
  Eigensolver = DivideAndConquer {}
  Filling = Fermi {
    Temperature [Kelvin] = 0.0
  }
  SlaterKosterFiles = Type2FileNames {
    Separator = "-"
    Suffix = ".skf"
  }
}

Analysis {
  CalculateForces = Yes
}

Options {
  WriteAutotestTag = Yes
}

ParserOptions {
  ParserVersion = 5
}

Parallel {
UseOmpThreads = Yes
}

